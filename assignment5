#Write Examples code for concatenation
a="1"
b="43"
print(a+b)
#Write one Examples for each formatting techniques
person = {"name": "Luffy", "age": 19, "city": "Windmill Village"}
print("My name is %(name)s, I am %(age)d years old, and I live in %(city)s." % person) 
#Go Through Reference site and apply various options into formatting techniques
price = 699
print("The price of tshirt is: Rs.%.2f" % price)
a=143
print("the value of a is: %d"%a)
b="hello"
print("the string: %s"%b)
#Write example for each arithmetic operators
a = 4
b = 6

# addition
print ('Sum: ', a + b)  

# subtraction
print ('Subtraction: ', a - b)   

# multiplication
print ('Multiplication: ', a * b)  

# division
print ('Division: ', a / b) 

# floor division
print ('Floor Division: ', a // b)

# modulo
print ('Modulo: ', a % b)  

# a to the power b
print ('Power: ', a ** b)   
#Write Example for assignment operators (except: = & +=)
a = 21
b = 11
c = 0

c *= a
print ("Line 1 - Value of c is ", c) 

c /= a 
print ("Line 2 - Value of c is ", c) 

c  = 2
c %= a
print ("Line 3 - Value of c is ", c)

c **= a
print ("Line 4 - Value of c is ", c)

c //= a
print ("Line 5 - Value of c is ", c)
